{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "./api/",
        "recurse": true,
        "readme": "README.md"
    },
    "templates": {
        "systemName": "MMEditor",
        "copyright": "@mizy.mz",
        "theme": "readable",
        "linenums": true,
        "collapseSymbols": true,
        "inverseNav": true,
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "search": true
    }
}